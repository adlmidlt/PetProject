<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="109.0" prefWidth="303.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ru.zavodsvet.fgw_desktop_v2.controller.menuBar.handbook.locStore.cardLocStore.QuantitySubLocStoreDialogViewController">
    <HBox>
        <Label text="Введите кол-во участков хранения ">
            <font>
                <Font size="18.0"/>
            </font>
            <HBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </HBox.margin>
        </Label>
    </HBox>
    <TextField fx:id="quantitySubLocStoreTF" layoutX="78.0" layoutY="34.0" prefHeight="25.0" prefWidth="137.0"/>
    <HBox layoutX="50.0" layoutY="73.0" prefHeight="43.0" prefWidth="194.0">
        <Button fx:id="createBtn" mnemonicParsing="false" prefHeight="30.0" prefWidth="80.0" text="Создать">
            <HBox.margin>
                <Insets right="15.0"/>
            </HBox.margin>
        </Button>
        <Button fx:id="cancelBtn" mnemonicParsing="false" prefHeight="30.0" prefWidth="80.0" text="Отмена">
            <HBox.margin>
                <Insets left="15.0"/>
            </HBox.margin>
        </Button>
    </HBox>
</AnchorPane>
