<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="140.0" prefWidth="354.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ru.zavodsvet.fgw_desktop_v2.controller.menuBar.operation.allOperation.DateTimeDialogViewController">

    <Label alignment="CENTER" text="На какую дату?" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
           AnchorPane.topAnchor="5.0">
        <font>
            <Font size="18.0"/>
        </font>
    </Label>

    <fx:include fx:id="templateDateTimeView" source="../../../template/TemplateDateTimeView.fxml"
                AnchorPane.bottomAnchor="70.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                AnchorPane.topAnchor="45.0"/>
    <Button fx:id="okBtn" layoutX="48.0" layoutY="96.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="80.0"
            text="ОК"/>
    <Button fx:id="cancelBtn" layoutX="220.0" layoutY="96.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="80.0"
            text="Отмена"/>

</AnchorPane>
